---
import Layout from '../layouts/Layout.astro';
---

<Layout title="Servicios - Tienda de Suplementos">
	<!-- Header (same as other pages) -->
	<header class="bg-black text-white">
		<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
			<div class="flex items-center justify-between h-16">
				<!-- Logo -->
				<div class="flex-shrink-0">
					<a href="/">
						<img src="/images/logo.png" alt="SUPLEMENTOS PRO" class="h-12 w-auto" />
					</a>
				</div>
				
				<!-- Navigation -->
				<nav class="hidden md:block">
					<ul class="flex space-x-8">
						<li><a href="/" class="hover:text-yellow-400 transition">Inicio</a></li>
						<li><a href="/productos" class="hover:text-yellow-400 transition">Productos</a></li>
						<li><a href="/ofertas" class="hover:text-yellow-400 transition">Ofertas</a></li>
						<li><a href="/marcas" class="hover:text-yellow-400 transition">Marcas</a></li>
						<li><a href="/objetivos" class="hover:text-yellow-400 transition">Objetivos</a></li>
						<li><a href="/autenticidad" class="hover:text-yellow-400 transition">Autenticidad</a></li>
						<li><a href="/blog" class="hover:text-yellow-400 transition">Blog</a></li>
					</ul>
				</nav>
				
				<!-- Search Bar -->
				<div class="hidden lg:block flex-1 max-w-md mx-8">
					<form class="relative">
						<input 
							type="search" 
							placeholder="Buscar proteínas, creatina, vitaminas..." 
							class="w-full bg-gray-800 text-white px-4 py-2 pr-10 rounded-lg focus:outline-none focus:ring-2 focus:ring-yellow-400 focus:bg-gray-700 transition"
						>
						<button type="submit" class="absolute right-2 top-1/2 transform -translate-y-1/2 hover:text-yellow-400 transition">
							<svg class="h-5 w-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"></path>
							</svg>
						</button>
					</form>
				</div>
				
				<!-- Cart and User -->
				<div class="flex items-center space-x-4">
					<button class="hover:text-yellow-400 transition">
						<svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path>
						</svg>
					</button>
					<a href="/carrito" class="hover:text-yellow-400 transition relative">
						<svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2.293 2.293c-.63.63-.184 1.707.707 1.707H17m0 0a2 2 0 100 4 2 2 0 000-4zm-8 2a2 2 0 11-4 0 2 2 0 014 0z"></path>
						</svg>
						<span class="absolute -top-2 -right-2 bg-yellow-400 text-black text-xs rounded-full h-5 w-5 flex items-center justify-center">3</span>
					</a>
				</div>
			</div>
		</div>
	</header>

	<main>
		<!-- Hero Section -->
		<section class="bg-gradient-to-br from-gray-900 via-blue-900 to-indigo-900 text-white relative overflow-hidden">
			<div class="absolute inset-0 bg-black/20"></div>
			<div class="relative max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-16">
				<div class="text-center">
					<div class="mb-6">
						<span class="inline-block bg-yellow-400/20 text-yellow-300 px-4 py-2 rounded-full text-sm font-semibold uppercase tracking-wide backdrop-blur-sm border border-yellow-400/30">
							Servicios Profesionales
						</span>
					</div>
					<h1 class="text-4xl md:text-6xl font-bold mb-6 bg-gradient-to-r from-white to-blue-200 bg-clip-text text-transparent">
						Servicios Especializados
					</h1>
					<p class="text-xl md:text-2xl text-blue-100 mb-8 max-w-3xl mx-auto">
						Asesoría nutricional personalizada y planes profesionales para maximizar tus resultados.
					</p>
					<div class="flex flex-col sm:flex-row gap-4 justify-center items-center">
						<button class="bg-yellow-400 text-gray-900 font-bold py-4 px-8 rounded-lg hover:bg-yellow-300 transition-all duration-300 transform hover:scale-105 shadow-lg">
							Ver Planes Disponibles
						</button>
						<button class="border-2 border-white/30 text-white font-semibold py-4 px-8 rounded-lg hover:bg-white/10 transition-all duration-300 backdrop-blur-sm">
							Consulta Gratuita
						</button>
					</div>
				</div>
			</div>
			<div class="absolute bottom-0 left-0 right-0 h-px bg-gradient-to-r from-transparent via-white/20 to-transparent"></div>
		</section>

		<!-- Stats Section -->
		<section class="py-16 bg-gray-50">
			<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
				<div class="grid grid-cols-3 gap-6 text-center">
					<div>
						<div class="text-3xl font-bold text-blue-600">500+</div>
						<div class="text-sm text-gray-600">Clientes Asesorados</div>
					</div>
					<div>
						<div class="text-3xl font-bold text-green-600">98%</div>
						<div class="text-sm text-gray-600">Satisfacción</div>
					</div>
					<div>
						<div class="text-3xl font-bold text-purple-600">5+</div>
						<div class="text-sm text-gray-600">Años Experiencia</div>
					</div>
				</div>
			</div>
		</section>

		<!-- Services Preview -->
		<section class="py-16">
			<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
				<h2 class="text-3xl font-bold text-center mb-12">Nuestros Servicios</h2>
				<div class="grid grid-cols-1 md:grid-cols-3 gap-8">
					<div class="bg-white p-6 rounded-lg shadow-lg text-center">
						<div class="w-16 h-16 bg-blue-100 rounded-full flex items-center justify-center mx-auto mb-4">
							<svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v6a2 2 0 002 2h2m0 0h2a2 2 0 002-2V7a2 2 0 00-2-2H9m0 8v2a2 2 0 002 2h2a2 2 0 002-2v-2"></path>
							</svg>
						</div>
						<h3 class="text-xl font-semibold mb-2">Plan Nutricional</h3>
						<p class="text-gray-600 mb-4">Alimentación personalizada según tus objetivos</p>
						<p class="text-2xl font-bold text-blue-600">$49</p>
					</div>
					<div class="bg-white p-6 rounded-lg shadow-lg text-center">
						<div class="w-16 h-16 bg-green-100 rounded-full flex items-center justify-center mx-auto mb-4">
							<svg class="w-8 h-8 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path>
							</svg>
						</div>
						<h3 class="text-xl font-semibold mb-2">Asesoría Personal</h3>
						<p class="text-gray-600 mb-4">Seguimiento individual con nutricionista</p>
						<p class="text-2xl font-bold text-green-600">$79</p>
					</div>
					<div class="bg-white p-6 rounded-lg shadow-lg text-center">
						<div class="w-16 h-16 bg-purple-100 rounded-full flex items-center justify-center mx-auto mb-4">
							<svg class="w-8 h-8 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
							</svg>
						</div>
						<h3 class="text-xl font-semibold mb-2">Plan de Entrenamiento</h3>
						<p class="text-gray-600 mb-4">Rutinas diseñadas por entrenadores</p>
						<p class="text-2xl font-bold text-purple-600">$39</p>
					</div>
				</div>
			</div>
		</section>

		<!-- Continue with remaining service sections -->
							<button class="bg-gradient-to-r from-yellow-400 to-orange-500 text-black font-bold py-4 px-8 rounded-lg hover:from-yellow-500 hover:to-orange-600 transition-all duration-300 transform hover:scale-105 shadow-lg">
								Consulta Gratis
							</button>
							<button class="border-2 border-white text-white font-bold py-4 px-8 rounded-lg hover:bg-white hover:text-gray-900 transition-all duration-300">
								Ver Servicios
							</button>
						</div>
						
						<!-- Trust Indicators -->
						<div class="mt-8 flex items-center justify-center lg:justify-start space-x-6 text-sm text-gray-400">
							<div class="flex items-center">
								<svg class="w-5 h-5 text-green-400 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
									<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
								</svg>
								Nutricionistas Certificados
							</div>
							<div class="flex items-center">
								<svg class="w-5 h-5 text-green-400 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
									<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
								</svg>
								Seguimiento 24/7
							</div>
						</div>
					</div>
					
					<!-- Visual Element -->
					<div class="relative">
						<!-- Main Image Container -->
						<div class="relative z-10 bg-gradient-to-br from-white to-gray-100 rounded-2xl p-8 shadow-2xl">
							<!-- Services Preview Cards -->
							<div class="space-y-4">
								<!-- Card 1 -->
								<div class="bg-gradient-to-r from-blue-500 to-purple-600 rounded-lg p-4 text-white">
									<div class="flex items-center space-x-3">
										<div class="w-10 h-10 bg-white bg-opacity-20 rounded-lg flex items-center justify-center">
											<svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
											</svg>
										</div>
										<div>
											<h3 class="font-semibold">Plan Nutricional</h3>
											<p class="text-sm opacity-90">Personalizado para tus objetivos</p>
										</div>
