---
import Layout from '../layouts/Layout.astro';
---

<Layout title="Carrito de Compras - Tienda de Suplementos">
	<!-- Header (same as other pages) -->
	<header class="bg-black text-white">
		<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
			<div class="flex items-center justify-between h-16">
				<!-- Logo -->
				<div class="flex-shrink-0">
					<a href="/">
						<img src="/images/logo.png" alt="SUPLEMENTOS PRO" class="h-12 w-auto" />
					</a>
				</div>
				
				<!-- Navigation -->
				<nav class="hidden md:block">
					<ul class="flex space-x-8">
						<li><a href="/" class="hover:text-yellow-400 transition">Inicio</a></li>
						<li><a href="/productos" class="hover:text-yellow-400 transition">Productos</a></li>
						<li><a href="/ofertas" class="hover:text-yellow-400 transition">Ofertas</a></li>
						<li><a href="/marcas" class="hover:text-yellow-400 transition">Marcas</a></li>
						<li><a href="/objetivos" class="hover:text-yellow-400 transition">Objetivos</a></li>
						<li><a href="/autenticidad" class="hover:text-yellow-400 transition">Autenticidad</a></li>
						<li><a href="/blog" class="hover:text-yellow-400 transition">Blog</a></li>
					</ul>
				</nav>
				
				<!-- Search Bar -->
				<div class="hidden lg:block flex-1 max-w-md mx-8">
					<form class="relative">
						<input 
							type="search" 
							placeholder="Buscar proteínas, creatina, vitaminas..." 
							class="w-full bg-gray-800 text-white px-4 py-2 pr-10 rounded-lg focus:outline-none focus:ring-2 focus:ring-yellow-400 focus:bg-gray-700 transition"
						>
						<button type="submit" class="absolute right-2 top-1/2 transform -translate-y-1/2 hover:text-yellow-400 transition">
							<svg class="h-5 w-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"></path>
							</svg>
						</button>
					</form>
				</div>
				
				<!-- Cart and User -->
				<div class="flex items-center space-x-4">
					<button class="hover:text-yellow-400 transition">
						<svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path>
						</svg>
					</button>
					<button class="text-yellow-400 relative">
						<svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2.293 2.293c-.63.63-.184 1.707.707 1.707H17m0 0a2 2 0 100 4 2 2 0 000-4zm-8 2a2 2 0 11-4 0 2 2 0 014 0z"></path>
						</svg>
						<span class="absolute -top-2 -right-2 bg-yellow-400 text-black text-xs rounded-full h-5 w-5 flex items-center justify-center">3</span>
					</button>
				</div>
			</div>
		</div>
	</header>

	<main>
		<!-- Breadcrumb -->
		<section class="bg-gray-100 py-3">
			<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
				<nav class="flex items-center space-x-2 text-sm">
					<a href="/" class="text-gray-600 hover:text-gray-900">Inicio</a>
					<span class="text-gray-400">/</span>
					<span class="text-gray-900 font-medium">Carrito de Compras</span>
				</nav>
			</div>
		</section>

		<!-- Shopping Cart -->
		<section class="py-8">
			<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
				<h1 class="text-3xl font-bold mb-8">Carrito de Compras</h1>
				
				<div class="grid grid-cols-1 lg:grid-cols-3 gap-8">
					<!-- Cart Items -->
					<div class="lg:col-span-2 space-y-6">
						<!-- Cart Item 1 -->
						<div class="bg-white rounded-lg border border-gray-200 p-6">
							<div class="flex items-center space-x-4">
								<div class="flex-shrink-0">
									<img src="/images/logo.png" alt="Whey Protein Gold" class="w-20 h-20 object-cover rounded-lg bg-gray-100" />
								</div>
								<div class="flex-1 min-w-0">
									<h3 class="text-lg font-semibold text-gray-900">
										<a href="/producto" class="hover:text-blue-600 transition">Whey Protein Gold Standard</a>
									</h3>
									<p class="text-sm text-gray-600">OPTIMUM NUTRITION</p>
									<div class="flex items-center space-x-4 mt-2">
										<span class="text-sm text-gray-600">Sabor: Chocolate</span>
										<span class="text-sm text-gray-600">Tamaño: 2.27kg</span>
									</div>
									<p class="text-sm text-green-600 mt-1">En stock</p>
								</div>
								<div class="flex flex-col items-end space-y-3">
									<!-- Price -->
									<div class="text-right">
										<p class="text-lg font-bold text-blue-600">$49.99</p>
										<p class="text-sm text-gray-500 line-through">$59.99</p>
									</div>
									
									<!-- Quantity Controls -->
									<div class="flex items-center space-x-2">
										<button class="w-8 h-8 rounded border border-gray-300 flex items-center justify-center hover:bg-gray-100 transition">
											<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M20 12H4"></path>
											</svg>
										</button>
										<span class="w-12 text-center border border-gray-300 py-1 rounded">1</span>
										<button class="w-8 h-8 rounded border border-gray-300 flex items-center justify-center hover:bg-gray-100 transition">
											<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"></path>
											</svg>
										</button>
									</div>
									
									<!-- Remove Button -->
									<button class="text-red-600 hover:text-red-800 text-sm font-medium transition">
										<svg class="w-4 h-4 inline mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16"></path>
										</svg>
										Eliminar
									</button>
								</div>
							</div>
						</div>

						<!-- Cart Item 2 -->
						<div class="bg-white rounded-lg border border-gray-200 p-6">
							<div class="flex items-center space-x-4">
								<div class="flex-shrink-0">
									<img src="/images/logo.png" alt="Creatina Monohidrato" class="w-20 h-20 object-cover rounded-lg bg-gray-100" />
								</div>
								<div class="flex-1 min-w-0">
									<h3 class="text-lg font-semibold text-gray-900">
										<a href="/producto" class="hover:text-blue-600 transition">Creatina Monohidrato</a>
									</h3>
									<p class="text-sm text-gray-600">OPTIMUM NUTRITION</p>
									<div class="flex items-center space-x-4 mt-2">
										<span class="text-sm text-gray-600">Sabor: Natural</span>
										<span class="text-sm text-gray-600">Tamaño: 300g</span>
									</div>
									<p class="text-sm text-green-600 mt-1">En stock</p>
								</div>
								<div class="flex flex-col items-end space-y-3">
									<!-- Price -->
									<div class="text-right">
										<p class="text-lg font-bold text-blue-600">$29.99</p>
									</div>
									
									<!-- Quantity Controls -->
									<div class="flex items-center space-x-2">
										<button class="w-8 h-8 rounded border border-gray-300 flex items-center justify-center hover:bg-gray-100 transition">
											<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M20 12H4"></path>
											</svg>
										</button>
										<span class="w-12 text-center border border-gray-300 py-1 rounded">2</span>
										<button class="w-8 h-8 rounded border border-gray-300 flex items-center justify-center hover:bg-gray-100 transition">
											<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"></path>
											</svg>
										</button>
									</div>
									
									<!-- Remove Button -->
									<button class="text-red-600 hover:text-red-800 text-sm font-medium transition">
										<svg class="w-4 h-4 inline mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16"></path>
										</svg>
										Eliminar
									</button>
								</div>
							</div>
						</div>

						<!-- Cart Item 3 -->
						<div class="bg-white rounded-lg border border-gray-200 p-6">
							<div class="flex items-center space-x-4">
								<div class="flex-shrink-0">
									<img src="/images/logo.png" alt="Pre-Workout Extreme" class="w-20 h-20 object-cover rounded-lg bg-gray-100" />
								</div>
								<div class="flex-1 min-w-0">
									<h3 class="text-lg font-semibold text-gray-900">
										<a href="/producto" class="hover:text-blue-600 transition">Pre-Workout Extreme</a>
									</h3>
									<p class="text-sm text-gray-600">C4 ORIGINAL</p>
									<div class="flex items-center space-x-4 mt-2">
										<span class="text-sm text-gray-600">Sabor: Frutas del Bosque</span>
										<span class="text-sm text-gray-600">Tamaño: 390g</span>
									</div>
									<p class="text-sm text-green-600 mt-1">En stock</p>
								</div>
								<div class="flex flex-col items-end space-y-3">
									<!-- Price -->
									<div class="text-right">
										<p class="text-lg font-bold text-blue-600">$39.99</p>
									</div>
									
									<!-- Quantity Controls -->
									<div class="flex items-center space-x-2">
										<button class="w-8 h-8 rounded border border-gray-300 flex items-center justify-center hover:bg-gray-100 transition">
											<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M20 12H4"></path>
											</svg>
										</button>
										<span class="w-12 text-center border border-gray-300 py-1 rounded">1</span>
										<button class="w-8 h-8 rounded border border-gray-300 flex items-center justify-center hover:bg-gray-100 transition">
											<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4"></path>
											</svg>
										</button>
									</div>
									
									<!-- Remove Button -->
									<button class="text-red-600 hover:text-red-800 text-sm font-medium transition">
										<svg class="w-4 h-4 inline mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16"></path>
										</svg>
										Eliminar
									</button>
								</div>
							</div>
						</div>

						<!-- Continue Shopping -->
						<div class="flex justify-between items-center">
							<a href="/" class="inline-flex items-center text-blue-600 hover:text-blue-700 font-medium transition">
								<svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
									<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7"></path>
								</svg>
								Continuar Comprando
							</a>
							<button class="text-gray-600 hover:text-gray-800 font-medium transition">
								Limpiar Carrito
							</button>
						</div>
					</div>

					<!-- Order Summary -->
					<div class="lg:col-span-1">
						<div class="bg-white rounded-lg border border-gray-200 p-6 sticky top-4">
							<h2 class="text-xl font-bold mb-6">Resumen del Pedido</h2>
							
							<!-- Order Items Summary -->
							<div class="space-y-3 mb-6">
								<div class="flex justify-between">
									<span class="text-gray-600">Whey Protein Gold × 1</span>
									<span class="font-medium">$49.99</span>
								</div>
								<div class="flex justify-between">
									<span class="text-gray-600">Creatina Monohidrato × 2</span>
									<span class="font-medium">$59.98</span>
								</div>
								<div class="flex justify-between">
									<span class="text-gray-600">Pre-Workout Extreme × 1</span>
									<span class="font-medium">$39.99</span>
								</div>
							</div>

							<div class="border-t border-gray-200 pt-4 space-y-3">
								<div class="flex justify-between">
									<span class="text-gray-600">Subtotal</span>
									<span class="font-medium">$149.96</span>
								</div>
								<div class="flex justify-between">
									<span class="text-gray-600">Descuento</span>
									<span class="font-medium text-green-600">-$10.00</span>
								</div>
								<div class="flex justify-between">
									<span class="text-gray-600">Envío</span>
									<span class="font-medium text-green-600">Gratis</span>
								</div>
								<div class="flex justify-between">
									<span class="text-gray-600">Impuestos</span>
									<span class="font-medium">$13.50</span>
								</div>
								<div class="border-t border-gray-200 pt-3">
									<div class="flex justify-between">
										<span class="text-lg font-bold">Total</span>
										<span class="text-xl font-bold text-blue-600">$153.46</span>
									</div>
								</div>
							</div>

							<!-- Promo Code -->
							<div class="mt-6">
								<label class="block text-sm font-medium text-gray-700 mb-2">Código de Descuento</label>
								<div class="flex space-x-2">
									<input 
										type="text" 
										placeholder="Ingresa tu código"
										class="flex-1 border border-gray-300 rounded-lg px-3 py-2 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-transparent"
									>
									<button class="bg-gray-900 text-white px-4 py-2 rounded-lg hover:bg-gray-800 transition">
										Aplicar
									</button>
								</div>
							</div>

							<!-- Shipping Options -->
							<div class="mt-6">
								<label class="block text-sm font-medium text-gray-700 mb-3">Opciones de Envío</label>
								<div class="space-y-2">
									<label class="flex items-center">
										<input type="radio" name="shipping" value="standard" checked class="text-blue-600 focus:ring-blue-500">
										<span class="ml-2 text-sm">
											<span class="font-medium">Envío Estándar</span>
											<span class="text-gray-600 block">3-5 días hábiles - Gratis</span>
										</span>
									</label>
									<label class="flex items-center">
										<input type="radio" name="shipping" value="express" class="text-blue-600 focus:ring-blue-500">
										<span class="ml-2 text-sm">
											<span class="font-medium">Envío Express</span>
											<span class="text-gray-600 block">1-2 días hábiles - $15.00</span>
										</span>
									</label>
								</div>
							</div>

							<!-- Checkout Button -->
							<button class="w-full bg-green-500 text-white font-bold py-3 px-6 rounded-lg hover:bg-green-600 transition mt-6">
								Proceder al Checkout
							</button>

							<!-- Security Badge -->
							<div class="mt-4 flex items-center justify-center space-x-2 text-xs text-gray-600">
								<svg class="w-4 h-4 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
									<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"></path>
								</svg>
								<span>Compra 100% segura</span>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>

		<!-- Recommended Products -->
		<section class="py-16 bg-gray-50">
			<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
				<h2 class="text-2xl font-bold mb-8">También te puede interesar</h2>
				<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6">
					<div class="bg-white border border-gray-200 rounded-lg overflow-hidden hover:shadow-lg transition">
						<a href="/producto" class="block">
							<div class="h-48 bg-gray-200 flex items-center justify-center">
								<span class="text-gray-400">[IMAGEN]</span>
							</div>
						</a>
						<div class="p-4">
							<a href="/producto" class="block hover:text-blue-600 transition">
								<h4 class="font-bold text-lg mb-2">BCAA 10:1:1</h4>
							</a>
							<p class="text-2xl font-bold text-blue-600 mb-3">$34.99</p>
							<button class="w-full bg-green-500 text-white font-bold py-2 rounded hover:bg-green-600 transition">Agregar al Carrito</button>
						</div>
					</div>
					<div class="bg-white border border-gray-200 rounded-lg overflow-hidden hover:shadow-lg transition">
						<a href="/producto" class="block">
							<div class="h-48 bg-gray-200 flex items-center justify-center">
								<span class="text-gray-400">[IMAGEN]</span>
							</div>
						</a>
						<div class="p-4">
							<a href="/producto" class="block hover:text-blue-600 transition">
								<h4 class="font-bold text-lg mb-2">Glutamina Pure</h4>
							</a>
							<p class="text-2xl font-bold text-blue-600 mb-3">$24.99</p>
							<button class="w-full bg-green-500 text-white font-bold py-2 rounded hover:bg-green-600 transition">Agregar al Carrito</button>
						</div>
					</div>
					<div class="bg-white border border-gray-200 rounded-lg overflow-hidden hover:shadow-lg transition">
						<a href="/producto" class="block">
							<div class="h-48 bg-gray-200 flex items-center justify-center">
								<span class="text-gray-400">[IMAGEN]</span>
							</div>
						</a>
						<div class="p-4">
							<a href="/producto" class="block hover:text-blue-600 transition">
								<h4 class="font-bold text-lg mb-2">Omega 3 Premium</h4>
							</a>
							<p class="text-2xl font-bold text-blue-600 mb-3">$19.99</p>
							<button class="w-full bg-green-500 text-white font-bold py-2 rounded hover:bg-green-600 transition">Agregar al Carrito</button>
						</div>
					</div>
					<div class="bg-white border border-gray-200 rounded-lg overflow-hidden hover:shadow-lg transition">
						<a href="/producto" class="block">
							<div class="h-48 bg-gray-200 flex items-center justify-center">
								<span class="text-gray-400">[IMAGEN]</span>
							</div>
						</a>
						<div class="p-4">
							<a href="/producto" class="block hover:text-blue-600 transition">
								<h4 class="font-bold text-lg mb-2">Multivitamínico Sport</h4>
							</a>
							<p class="text-2xl font-bold text-blue-600 mb-3">$22.99</p>
							<button class="w-full bg-green-500 text-white font-bold py-2 rounded hover:bg-green-600 transition">Agregar al Carrito</button>
						</div>
					</div>
				</div>
			</div>
		</section>
	</main>

	<!-- Footer (same as other pages) -->
	<footer class="bg-gray-900 text-gray-300">
		<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
			<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
				<div>
					<img src="/images/logo.png" alt="SUPLEMENTOS PRO" class="h-16 w-auto mb-4" />
					<p class="text-sm mb-4">Tu tienda de confianza para suplementos deportivos de alta calidad.</p>
				</div>
				<div>
					<h4 class="font-bold text-white text-lg mb-4">PRODUCTOS</h4>
					<ul class="space-y-2">
						<li><a href="#" class="hover:text-white transition">Proteínas</a></li>
						<li><a href="#" class="hover:text-white transition">Pre-Entrenos</a></li>
						<li><a href="#" class="hover:text-white transition">Creatina</a></li>
						<li><a href="#" class="hover:text-white transition">Vitaminas</a></li>
